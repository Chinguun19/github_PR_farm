# GitHub credentials
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_USERNAME=your_github_username

# PR Farm Configuration
# Number of repositories to create
PR_FARM_REPO_COUNT=5
# Base name for repositories
PR_FARM_REPO_NAME=pr-farm-repo
# Number of PRs per repository
PR_FARM_PRS_PER_REPO=3
# Whether to automatically merge PRs
PR_FARM_AUTO_MERGE=true
# Delay between operations (in ms)
PR_FARM_DELAY=2000

# Scheduler Configuration
# Cron schedule for PR creation (default: every day at 10:00 AM)
PR_FARM_CREATE_SCHEDULE="0 10 * * *"
# Cron schedule for PR review (default: every day at 2:00 PM)
PR_FARM_REVIEW_SCHEDULE="0 14 * * *"
# Whether to run tasks on scheduler startup
PR_FARM_RUN_ON_STARTUP=true 